            "beta_1": 0.9,
            "beta_2": 0.999,
            "epsilon": 0.00000001,
            "decay": 0.0
        },
        "compile": {
            "loss": "binary_crossentropy",
            "optimizer": "adam",
            "metrics": ["accuracy"]
        },
        "fit": {
            "batch_size": 64,
            "epochs": 30,
            "validation_split": 0.2
        },
        "pipeline": {
            "tokenizer": null
        },
        "params": {
            "verbose": 2
        },
        "label_encoder": null
    }
